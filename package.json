{
  "name": "tsunit",
  "version": "0.1.0",
  "description": "An xUnit test framework written with TypeScript that follows and builds on Kent Beck's example in Test-Driven Development: By Example",
  "scripts": {
    "start": "npm test",
    "pretest": "tsc",
    "test": "node ./bin/tsunit --exclude=dist/test/WasRun.js dist/test",
    "install": "tsc"
  },
  "bin": "./bin/tsunit.js",
  "keywords": [
    "xunit",
    "tsunit",
    "tdd",
    "testing",
    "typescript"
  ],
  "author": {
    "name": "Sean Poulter",
    "email": "sean.poulter@gmail.com",
    "url": "https://github.com/seanpoulter"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/seanpoulter/tsunit"
  },
  "license": "ISC",
  "dependencies": {
    "@types/node": "^8.0.1",
    "typescript": "^2.3.4"
  }
}
